{
    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "matteocollina/mosca:latest",
            "forcePullImage": true,
            "network": "BRIDGE",
            "portMappings": [{
                "containerPort": 1883,
                "hostPort": 0
            }]
        },
        "forcePullImage": true
    },
    "healthChecks": [{
        "protocol": "TCP",
        "portIndex": 0
    }],
    "id": "/DEPLOYMENTNAME/mosca",
    "instances": 1,
    "cpus": 0.2,
    "mem": 64,
    "env": {}
}
