{
    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "hpreston/myhero_data:latest",
            "forcePullImage": true,
            "network": "BRIDGE",
            "portMappings": [{
                "containerPort": 5000,
                "hostPort": 0
            }]
        },
        "forcePullImage": true
    },
    "healthChecks": [{
        "protocol": "TCP",
        "portIndex": 0
    }],
    "id": "/DEPLOYMENTNAME/data",
    "instances": 1,
    "cpus": 0.2,
    "mem": 64,
    "env": {
        "myhero_data_key": "SecureData"
    }
}
