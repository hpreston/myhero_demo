{
    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "hpreston/myhero_web",
            "network": "BRIDGE",
            "portMappings": [{
                "containerPort": 5000,
                "hostPort": 0
            }]
        },
        "forcePullImage": true
    },
    "healthChecks": [{
        "protocol": "TCP",
        "portIndex": 0
    }],
    "id": "/myhero/web",
    "instances": 3,
    "cpus": 0.5,
    "mem": 32,
    "env": {
        "myhero_app_server": "http://myhero-app"
    }
}
